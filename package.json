{
  "name": "albertus",
  "description": "King Albert patience for the command line",
  "version": "0.1.0",
  "author": "Matthew Harvey @matt-harvey",
  "bin": {
    "albertus": "./bin/run"
  },
  "bugs": "https://github.com/matt-harvey/albertus/issues",
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "readline-promise": "^1.0.4",
    "runtypes": "^4.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^10.14.13",
    "jest": "^24.8.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "homepage": "https://github.com/matt-harvey/albertus",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "matt-harvey/albertus",
  "scripts": {
    "bin": "$npm_package_bin_albertus",
    "bin:compiled": "$npm_package_bin_albertus --run-compiled",
    "build": "tsc --build",
    "build:clean": "npm run clean && npm run build",
    "build:pack": "rm -rf pack && mkdir -p pack && npm run build && npm pack && mv albertus*.tgz pack/",
    "build:watch": "tsc --build --watch",
    "clean": "rm -rf dist && rm -rf pack && rm -f tsconfig.tsbuildinfo",
    "install:dev": "npm run build:pack && npm install ./pack/albertus*.tgz -g",
    "test": "jest --coverage",
    "posttest": "tslint --project . --format stylish",
    "start": "npm run bin"
  },
  "types": "dist/index.d.ts"
}
